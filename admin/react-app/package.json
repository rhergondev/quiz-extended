{
  "name": "quiz-extended",
  "version": "1.0.0",
  "description": "Tutor LMS extension for advanced questionnaires",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run css:build && wp-scripts build",
    "start": "npm run css:watch & wp-scripts start",
    "dev": "npm run css:watch & wp-scripts start",
    "css:build": "npx tailwindcss -i ./src/styles/tailwind-input.css -o ./src/styles/tailwind.css --minify",
    "css:watch": "npx tailwindcss -i ./src/styles/tailwind-input.css -o ./src/styles/tailwind.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhergondev/quiz-extended.git"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "quiz",
    "tutor-lms"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rhergondev/quiz-extended/issues"
  },
  "homepage": "https://github.com/rhergondev/quiz-extended#readme",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-table": "^8.21.3",
    "@wordpress/element": "^6.8.0",
    "lucide-react": "^0.539.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@wordpress/scripts": "^30.21.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.11"
  }
}
